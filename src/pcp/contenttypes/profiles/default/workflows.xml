<?xml version="1.0"?>
<object name="portal_workflow">
 <object name="community_workflow" meta_type="Workflow"/>
 <object name="project_workflow" meta_type="Workflow"/>
 <object name="offer_workflow" meta_type="Workflow"/>
 <object name="request_workflow" meta_type="Workflow"/>
 <object name="rolerequest_workflow" meta_type="Workflow"/>
 <bindings>
  <type type_id="Community">
   <bound-workflow workflow_id="community_workflow"/>
  </type>
  <type type_id="Project">
   <bound-workflow workflow_id="project_workflow"/>
  </type>
  <type type_id="ResourceOffer">
    <bound-workflow workflow_id="offer_workflow"/>
   </type>
   <type type_id="ServiceComponentOffer">
    <bound-workflow workflow_id="offer_workflow"/>
  </type>
  <type type_id="ResourceRequest">
   <bound-workflow workflow_id="request_workflow"/>
  </type>
  <type type_id="ServiceComponentRequest">
   <bound-workflow workflow_id="request_workflow"/>
  </type>
  <type type_id="ServiceRequest">
   <bound-workflow workflow_id="request_workflow"/>
  </type>
  <type type_id="RoleRequest">
   <bound-workflow workflow_id="rolerequest_workflow"/>
  </type>
 </bindings>
</object>
