<?xml version="1.0"?>
<object name="community_dx" meta_type="Dexterity FTI" i18n:domain="plone"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Customer</property>
 <property name="description" 
   i18n:translate="">A customer served by one or more providers through a project on this site.</property>
 <property name="icon_expr">string:${portal_url}/group.png</property>
 <property name="factory">community</property>
 <property name="add_view_expr">string:${folder_url}/++add++community_dx</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
   <element value="Document"/>
   <element value="File"/>
   <element value="Image"/>
   <element value="Folder"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">compact-view</property>
 <property name="view_methods">
  <element value="compact-view"/>
  <element value="view"/>
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">pcp.contenttypes.addCommunity</property>
 <property name="klass">pcp.contenttypes.content_dx.community.Community</property>
 <property name="schema">pcp.contenttypes.content_dx.community.ICommunity</property>
 <property name="behaviors">
  <element value="plone.dublincore"/>
  <element value="plone.namefromtitle"/>
  <element value="plone.versioning" />
  <element value="plone.excludefromnavigation" />
  <element value="plone.richtext" />
  <element value="plone.relateditems" />
  <element value="plone.locking" />
  <element value="DPMT.common" />
 </property>
 <property name="model_source"></property>
 <property name="model_file"></property>
 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
