<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="pcp.contenttypes"
    purge="False">

    <configlet
        title="DPMT Settings"
        action_id="pcp.contenttypes"
        appId="pcp.contenttypes"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@pcp-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
